{
  "name": "@benzn/promisify",
  "version": "0.0.1",
  "description": "Promisify utility for function and object",
  "type": "module",
  "main": ".dist/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "start": "node ./dist/index.js",
    "test": "tsx test/test.ts",
    "prebuild": "npm run check && npm run lint",
    "build": "tsup",
    "build:tsc": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --module CommonJS --outDir out/cjs",
    "build:esm": "tsc --module ESNext --outDir out/esm",
    "tsup": "tsup",
    "type-check": "tsc --noEmit",
    "lint": "eslint ./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benzaria/es-promisify.git"
  },
  "author": "benzaria",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benzaria/es-promisify/issues"
  },
  "homepage": "https://benzaria.github.io/es-promisify",
  "keywords": [
    "promisify",
    "utility",
    "eventListener",
    "function",
    "object"
  ],
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0"
  },
  "dependencies": {
    "@benzn/to-ms": "^1.2.0"
  }
}